{
	"extends": ["airbnb-base", "prettier"],
	"plugins": ["prettier"],
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module"
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"no-console": "off",
		"no-param-reassign": "off",
		"no-underscore-dangle": "off",
		"no-plusplus": "off"
	}
}
